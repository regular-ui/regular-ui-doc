.m-part {
    $box-sizing: border-box;
    display: inline-block;
    vertical-align: top;
    padding: 60px 18px;
    
    h2 {
        text-align: center;
        font-size: 16px;
        margin: 8px 0;

        > i {
            display: none;
        }
    }

    p {
        margin: 0;
        font-size: 14px;
        color: #888;
    }
}